{
  "session": {
    "trust_all_worktrees": true
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "auto_update": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte (Blur)",
    "dark": "Catppuccin Mocha (Blur)"
  },
  "icon_theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "ui_font_size": 16,
  "buffer_font_family": "MonoLisa Variable",
  "buffer_font_size": 14,
  "buffer_font_features": {
    "calt": true,
    "liga": true,
    "zero": true
  },
  "buffer_line_height": "comfortable",
  "tab_size": 2,
  "soft_wrap": "editor_width",
  "preferred_line_length": 100,
  "show_wrap_guides": true,
  "relative_line_numbers": "enabled",
  "show_whitespaces": "selection",
  "format_on_save": "on",
  "ensure_final_newline_on_save": true,
  "remove_trailing_whitespace_on_save": true,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true
  },
  "project_panel": {
    "git_status": true,
    "dock": "left",
    "auto_fold_dirs": true,
    "indent_size": 16
  },
  "terminal": {
    "font_family": "MonoLisa Variable",
    "font_size": 14,
    "option_as_meta": true,
    "copy_on_select": true,
    "blinking": "on"
  },
  "git": {
    "inline_blame": {
      "enabled": true,
      "delay_ms": 100,
      "min_column": 80
    }
  },
  "preview_tabs": {
    "enabled": true,
    "enable_preview_from_file_finder": true
  },
  "hover_popover_enabled": true
}
