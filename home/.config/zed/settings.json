{
  "session": {
    "trust_all_worktrees": true
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte (Blur)",
    "dark": "Catppuccin Mocha (Blur)"
  },
  "icon_theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "agent": {
    "play_sound_when_agent_done": true
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "base_keymap": "VSCode",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_family": "MonoLisa Variable",
  "buffer_font_size": 14,
  "buffer_font_features": {
    "calt": true,
    "liga": true,
    "zero": true,
    "onum": true,
    "ss01": true,
    "ss03": true,
    "ss04": true,
    "ss05": true,
    "ss06": true,
    "ss07": true,
    "ss08": true,
    "ss09": true,
    "ss10": true,
    "ss11": true,
    "ss12": true,
    "ss13": true,
    "ss14": true,
    "ss15": true,
    "ss16": true,
    "ss17": true,
    "ss18": true
  },
  "tab_size": 2,
  "remove_trailing_whitespace_on_save": true,
  "show_whitespaces": "selection",
  "format_on_save": "on",
  "ensure_final_newline_on_save": true,
  "soft_wrap": "preferred_line_length",
  "preferred_line_length": 100,
  "relative_line_numbers": true,
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true
  },
  "project_panel": {
    "git_status": true,
    "dock": "left",
    "auto_fold_dirs": true,
    "indent_size": 12
  },
  "terminal": {
    "font_family": "MonoLisa Variable",
    "font_size": 14,
    "option_as_meta": true,
    "copy_on_select": true,
    "blinking": "on"
  },
  "git": {
    "inline_blame": {
      "enabled": true,
      "delay_ms": 100,
      "min_column": 80
    }
  },
  "auto_update": true,
  "preview_tabs": {
    "enable_preview_from_file_finder": true
  },
  "hover_popover_enabled": true
}
